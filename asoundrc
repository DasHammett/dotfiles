#defaults.pcm.!card Generic
#defaults.pcm.!device 1
#defaults.pcm!ctl Generic


#pcm.!default {
#        type hw
#        card 0
#}
#
#ctl.!default {
#        type hw
#        card 0
#}
pcm.dmixed {
type dmix
    ipc_key 1024 # Any unique value for Interprocess Communication
    slave {
        pcm "hw:0,0"
        period_size 1024
        buffer_size 4096
        rate 48000
    }
}

pcm.asymed {
	type asym
	playback.pcm "dmixed"
	capture.pcm "hw:0,0"
}

pcm.!default {
	type plug
	slave.pcm "asymed"
	}
